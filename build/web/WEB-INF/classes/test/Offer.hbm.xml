<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping>
   <class name="test.Offer" table="offers">
      <id name="ido" type="int" column="ido">
         <generator class="native"/>
      </id>
      <property name="enterprise" column="enterprise" type="string"/>
      <property name="dateCreation" column="date_creation" type="date"/>
      <property name="dateBeginning" column="date_beginning" type="date"/>
      <property name="length" column="length" type="int"/>
      <property name="activityField" column="activity_field" type="string"/>
      <property name="title" column="title" type="string"/>
      <property name="areaCode" column="area_code" type="int"/>
      <property name="location" column="location" type="string"/>
      <property name="missionStatement" column="mission_statement" type="string"/>
      <property name="pay" column="pay" type="int"/>
      <property name="details" column="details" type="string"/>
      <property name="profile" column="profile" type="string"/>
      <property name="contactInformations" column="contact_informations" type="string"/>
   </class>
</hibernate-mapping>